{
 "display_name": "PySpark",
 "language": "python",
 "argv": [
  "python",
  "-m",
  "ipykernel_launcher",
  "-f",
  "{connection_file}"
 ],
 "env": {
  "SPARK_HOME": "/spark",
  "PYTHONPATH": "/spark/python:/spark/python/lib/py4j-0.10.6-src.zip",
  "PYTHONSTARTUP": "/spark/python/pyspark/shell.py",
  "PYSPARK_SUBMIT_ARGS": "pyspark-shell"
 }
}
